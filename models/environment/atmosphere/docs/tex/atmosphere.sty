\NeedsTeXFormat{LaTeX2e}% LaTeX 2.09 can't be used (nor non-LaTeX)
[1994/12/01]% LaTeX date must be December 1994 or later

% Package definition.
% Change atmosphere to package name. Change date and version to reflect updates.
%  (this may be done in compy_templates)
\ProvidesPackage{atmosphere}[2006/07/26 v1.1 atmosphere module macros]


% Package options. Change atmosphere to the value of \modelprefix (see below).
%  (this may be done in copy_templates)
\newif\ifatmosphereReqt@
\newif\ifatmosphereIVV@
\DeclareOption{Reqt}{\atmosphereReqt@true}
\DeclareOption{IVV}{\atmosphereIVV@true}
\ProcessOptions\par

% Include requirement labels if requested
\ifatmosphereReqt@
\input{atmosphereReqt.def}
\fi

% Include IV&V labels if requested
\ifatmosphereIVV@
\input{atmosphereIVV.def}
\fi

% Include the makefile-generated paths definitions file
\input{paths.def}

% use the better float package
\usepackage{float}

% use equation package stuff
\usepackage{amsmath, amsthm, amssymb}

% Define any other model-specific LaTeX commands here.

% \modelprefix - a short, unique, upper-case prefix used to tag requirements.
% THE atmosphere PREFIX MUST CONTAIN ONLY LETTERS.
% Examples: TIME for the universal time package, MASS for the mass tree.
\newcommand\modelprefix{atmosphere}
% \atmosphereDesc should be changed to a short acronym for the model, this may be
% done in copy_templates
% {MODEL DESCRIPTION} should be changed to the full name of the model.
%   For consistency with the text, the name should be a noun, possibly
%   finishing with the word "Model", e.g. "Time Model", not "Time"  ("The Time
%   Model works by ..."  vs "The Time works by").
%   The name should be a singular, not plural value, e.g. "Reference Frame Model",
%   not "Reference Frames" (same example).
\newcommand{\atmosphereDesc}{Atmosphere Model}
\newcommand{\ModelAuthor}{Zu Qun Li \& Christina Chomel}
\newcommand{\ModelAuthDate}{August, 2021}

% for all editors; add keywords as necessary.
\newcommand{\ModelKeywords}{\atmosphereDesc}

